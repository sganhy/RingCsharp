<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <LangVersion>10.0</LangVersion>
    <Platforms>AnyCPU;x64</Platforms>
    <AnalysisLevel>6.0-all</AnalysisLevel>
    <AssemblyVersion>1.0.2.0</AssemblyVersion>
    <FileVersion>1.0.2.0</FileVersion>
    <RepositoryType>https://github.com/sganhy/RingCsharp.git</RepositoryType>
    <RepositoryUrl>https://github.com/sganhy/RingCsharp</RepositoryUrl>
    <Description>Ring</Description>
    <Title>Ring</Title>
    <NoWarn>CA1307;CA1802</NoWarn>
    <AllowUnsafeBlocks>True</AllowUnsafeBlocks>
    <SignAssembly>True</SignAssembly>
    <AssemblyOriginatorKeyFile>Ring.snk</AssemblyOriginatorKeyFile>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <IsAotCompatible>True</IsAotCompatible>
    <DebugType>full</DebugType>
    <WarningLevel>6</WarningLevel>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|x64'">
    <IsAotCompatible>True</IsAotCompatible>
    <DebugType>full</DebugType>
    <WarningLevel>6</WarningLevel>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <IsAotCompatible>True</IsAotCompatible>
    <DebugType>full</DebugType>
    <WarningLevel>6</WarningLevel>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|x64'">
    <IsAotCompatible>True</IsAotCompatible>
    <DebugType>full</DebugType>
    <WarningLevel>6</WarningLevel>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Util\Resources\LogMessage.txt" />
    <None Remove="Util\Resources\MySQLReservedKeyWord.gz" />
    <None Remove="Util\Resources\OracleReservedKeyWord.gz" />
    <None Remove="Util\Resources\PostgreSQLReservedKeyWord.gz" />
    <None Remove="Util\Resources\SQLiteReservedKeyWord.gz" />
    <None Remove="Util\Resources\SQLServerReservedKeyWord.gz" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Util\Resources\LogMessage.txt" />
    <EmbeddedResource Include="Util\Resources\MySQLReservedKeyWord.gz" />
    <EmbeddedResource Include="Util\Resources\OracleReservedKeyWord.gz" />
    <EmbeddedResource Include="Util\Resources\PostgreSQLReservedKeyWord.gz" />
    <EmbeddedResource Include="Util\Resources\SQLiteReservedKeyWord.gz" />
    <EmbeddedResource Include="Util\Resources\SQLServerReservedKeyWord.gz" />
  </ItemGroup>

  <ItemGroup>
	  <InternalsVisibleTo Include="Ring.Tests" PublicKey="" />
	  <InternalsVisibleTo Include="Ring.Console" PublicKey="" />
  </ItemGroup>
 
</Project>
